{
    "$id": "Oscillator",
    "type": "object",
    "properties": {
        "mode": {
            "type": "string",
            "enum": ["basic", "fat", "am", "fm"],
            "default": "poly"
        },
        "type": {
            "type": "string",
            "enum": ["sine", "square", "sawtooth", "triangle", "pwm"],
            "default": "sine"
        },
        "frequency": {
            "type": "number",
            "multipleOf": 1,
            "default": 440,
            "minimum": 0,
            "maximum": 20000
        },
        "phase": {
            "type": "number",
            "multipleOf": 0.01,
            "default": 0,
            "minimum": -1,
            "maximum": 1
        },
        "detune": {
            "type": "number",
            "multipleOf": 1,
            "default": 0,
            "minimum": -100,
            "maximum": 100
        }
    },
    "additionalProperties": false
}
